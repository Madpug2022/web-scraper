generator client {
  provider = "prisma-client-js"
  output   = "./generated/mongo_client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Data {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  pageUrl   String
  pageTitle String
  imageUrl  String
  pageText  String
  createdAt DateTime @default(now())
}
